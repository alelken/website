# Firebase config
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# Feature toggles
# Show jobs section on Careers page when true
VITE_ENABLE_JOBS=false

# Theme config
# Enable/disable the light/dark toggle in the UI (true|false)
VITE_ENABLE_THEME_TOGGLE=true
# Default theme if toggle is disabled or first visit (light|dark)
VITE_DEFAULT_THEME=light

# Prerender config
# Absolute origin used to emit og:url and twitter:image during prerender
# Example: https://www.alelken.in or https://alelken.in
PRERENDER_ORIGIN=https://alelken.in
